{
  "expo": {
    "name": "Nidas Fashion",
    "slug": "nidas",
    "scheme": "nidas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nidas.fashion"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nidas.fashion",
      "permissions": [
                "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "versionCode": 1,
      "softwareKeyboardLayoutMode": "pan"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff"
        }
      ],
              [
          "expo-location",
          {
            "locationAlwaysAndWhenInUsePermission": "Allow Nidas Fashion to use your location."
          }
        ]
      ],
    "extra": {
      "eas": {
        "projectId": "f9494fc2-6728-43c8-b8b6-07582326ceb7"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f9494fc2-6728-43c8-b8b6-07582326ceb7"
    }
  }
}
